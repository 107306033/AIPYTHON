   "source": [
    "scores = input('Please input the scores(split with spaces between):')\n",
    "scoresList = scores.split()\n",
    "total = 0\n",
    "count = 0\n",
    "for i in range(len(scoresList)):\n",
    "    score = int(scoresList[i])\n",
    "    total += score\n",
    "    count += 1\n",
    "print('The average score is :', total/count)"
